<!DOCTYPE html>
<html class="desktop landscape" lang="{{ app.request.locale|split('_')|first|default('en') }}">
   <head>
      <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
      <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
      <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
      {% set global_configs = global_configs() %}
      <title>{% block page_title %}{{ global_configs.wsTitle }}{% endblock %}</title>
      <link rel="canonical" href="https://sunnyflowers.org/">
      <link href="#favicon.ico" rel="shortcut icon" type="image/x-icon">

      {% block head_stylesheets %}
         <link href="{{ asset('assets/css/bootstrap.css') }}" rel="stylesheet" type="text/css" media="all">
         <link href="{{ asset('assets/css/assets.css') }}" rel="stylesheet" type="text/css" media="all">
         <link href="{{ asset('assets/css/'~global_configs.theme_style) }}" rel="stylesheet" type="text/css" media="all">
         <link href="{{ asset('assets/css/responsive.css') }}" rel="stylesheet" type="text/css" media="all">
         <link href="{{ asset('assets/css/font-awesome.css') }}" rel="stylesheet" type="text/css" media="all">
         <link href="{{ asset('assets/css/animate.css') }}" rel="stylesheet" type="text/css" media="all">
         <link rel="stylesheet" href="{{ asset('assets/css/theme.css') }}" type="text/css">
      {% endblock %}

      {% block head_javascript %}
         <script src="{{ asset('assets/js/jquery.min.js') }}" type="text/javascript"></script>
         <script src="{{ asset('assets/js/jquery-migrate-1.2.1.min.js') }}" type="text/javascript"></script>
         <script src="{{ asset('assets/js/api.jquery.js') }}" type="text/javascript"></script>
         <script src="{{ asset('assets/js/device.min.js') }}" type="text/javascript"></script>
         <script src="{{ asset('assets/js/jquery.easing.1.3.js') }}" type="text/javascript"></script>
         <script src="{{ asset('assets/js/ajaxify-shop.js') }}" type="text/javascript"></script>
         <script src="{{ asset('assets/js/hoverIntent.js') }}" type="text/javascript"></script>
         <script src="{{ asset('assets/js/superfish.js') }}" type="text/javascript"></script>
         <script src="{{ asset('assets/js/sftouchscreen.js') }}" type="text/javascript"></script>
         <script src="{{ asset('assets/js/tm-stick-up.js') }}" type="text/javascript"></script>
         <script src="{{ asset('assets/js/jquery.ui.totop.js') }}" type="text/javascript"></script>
         <script src="{{ asset('assets/js/jquery.formstyler.min.js') }}" type="text/javascript"></script>
         <script src="{{ asset('assets/js/wow.min.js') }}" type="text/javascript"></script>
         <script src="{{ asset('assets/js/jquery.nivoslider.js') }}" type="text/javascript"></script>
         <script src="{{ asset('assets/js/jquery.bxslider.min.js') }}" type="text/javascript"></script>
      {% endblock %}
   </head>

   {% block body %}
   <body id="{{ body_id|default('flowers-store')}}" class="{{ body_class|default('template-index')}}">
      <!--[if lt IE 9]>
      <div class="old_browsers"><a href="//windows.microsoft.com/en-us/internet-explorer/download-ie"><i class="fa fa-warning"></i>You are using an outdated version of Internet Explorer. Upgrade today for a faster, safer browsing experience.</a></div>
      <![endif]-->
      {% block wrapper %}
      <div id="wrapper">
         <div id="wrapper2">
            <!-- HEADER -->
            {% block header %}
            <header id="header">
               <div class="container">
                  {% block header_top %}
                     {{ include('FrontBundle:Default:header_top.html.twig') }}
                  {% endblock %}
                  {% block header_main %}
                     {{ include('FrontBundle:Default:header_main.html.twig') }}
                  {% endblock %}
               </div>
            </header>
            {% endblock %}

            <!-- MEGAMENU -->
            {% block megamenu %}
               {{ include('FrontBundle:Default:megamenu.html.twig') }}
            {% endblock %}

            <!-- NIVOSLIDER -->
            {% block nivoslider %}
               {{ include('FrontBundle:Default:nivoslider.html.twig') }}
            {% endblock %}

            {% block showcase %}
               {{ include('FrontBundle:Default:showcase.html.twig') }}
            {% endblock %}

            <!-- MAIN CONTENT -->
            {% block main %}
                <!-- Content here... -->
            {% endblock %}

            <!-- Custom bottom -->
            {% block custom_bottom %}
               {{ include('FrontBundle:Default:custom_bottom.html.twig') }}
            {% endblock %}

            <!-- FOOTER -->
            {% block footer %}
               {{ include('FrontBundle:Default:footer.html.twig') }}
            {% endblock %}

         </div>
         <!-- / #wrapper2 -->
      </div>
      {% endblock %}
      <!-- / #wrapper -->
      {% block body_javascript %}
         <script src="{{ asset('assets/js/shop.js') }}" type="text/javascript"></script>
         <script src="{{ asset('assets/js/jquery.parallax.js') }}" type="text/javascript"></script>
      {% endblock %}
   </body>
   {% endblock %}
</html>